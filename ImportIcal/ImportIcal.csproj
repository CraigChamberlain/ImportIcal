<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>ImportIcal</PackageId>
    <Version>0.0.1</Version>
    <Authors>Craig Chamberlain</Authors>
    <Company>Craig Chamberlain IT</Company>
    <Product>PowerShell Module</Product>
    <PackageTags>PSModule</PackageTags>
    <PackageProjectUrl>https://www.powershellgallery.com/packages/ImportIcal</PackageProjectUrl>
    <RepositoryUrl>https://github.com/CraigChamberlain/ImportIcal</RepositoryUrl>
    <RepositoryType></RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Ical.Net" Version="4.3.1" />
    <PackageReference Include="System.Management.Automation" Version="7.3.12" />
  </ItemGroup>

  <ItemGroup>
    <None Update="ImportIcal.psd1">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="ImportScript.ps1">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
